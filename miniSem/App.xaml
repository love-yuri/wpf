<Application x:Class="miniSem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:miniSem"
             xmlns:utils="clr-namespace:miniSem.Base.Utils"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/BaseStyle.xaml" />
                <utils:ThemeStyleResource Source="/Styles/LightStyle.xaml" />
                <utils:LanguageResource Source="/Properties/zh-cn.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
